  CREATE TABLE "AM_USER"
   (	"USER_ID" NUMBER(10,0)  IDENTITY PRIMARY KEY,
	"ADDRESS" VARCHAR2(512 CHAR),
	"AUTH_METHOD" VARCHAR2(20 CHAR),
	"CREATED_ID" NUMBER(10,0),
	"CREATED_TIME" DATE,
	"DELETED_ID" NUMBER(10,0),
	"EMAIL" VARCHAR2(50 CHAR),
	"EXPIRE_STATUS" NUMBER(1,0),
	"FAILURE_COUNT" NUMBER(5,0),
	"FAX" VARCHAR2(15 CHAR),
	"FULL_NAME" VARCHAR2(100 CHAR),
	"LOCKED_DATE" DATE,
	"MOBILE" VARCHAR2(15 CHAR),
	"MODIFIED_ID" NUMBER(10,0),
	"MODIFIED_PASSWORD" DATE,
	"PASSWORD" VARCHAR2(100 CHAR),
	"PASSWORD_BACKUP" VARCHAR2(50 CHAR),
	"PHONE" VARCHAR2(15 CHAR),
	"PRIORITY" NUMBER(5,0),
	"RIGHT_TYPE" NUMBER(19,0),
	"STATUS" NUMBER(1,0),
	"USER_NAME" VARCHAR2(50 CHAR),
	"DOCUMENT" CLOB
   )

     CREATE TABLE "SE_RESOURCE"
      (	"NAME" VARCHAR2(512 CHAR)  IDENTITY PRIMARY KEY,
   	"CREATED_TIME" DATE,
   	"STATUS" NUMBER(1,0)
      )

        CREATE TABLE "SE_ROLE"
         (	"NAME" VARCHAR2(512 CHAR)  IDENTITY PRIMARY KEY,
      	"CREATED_TIME" DATE,
      	"STATUS" NUMBER(1,0),
      "DESCRIPTION" VARCHAR2(512 CHAR) )


      CREATE TABLE "SE_PERMISSION"
         (	"NAME" VARCHAR2(50 CHAR)  IDENTITY PRIMARY KEY,
      	"CREATED_TIME" DATE,
      	"STATUS" NUMBER(1,0),
	"RESOURCE" VARCHAR2(512 CHAR) ,
	"ACTION" VARCHAR2(30 CHAR) ,
FOREIGN KEY ( RESOURCE ) REFERENCES SE_RESOURCE( NAME ),
FOREIGN KEY ( ACTION ) REFERENCES SE_ACTION( CODE )

         )

    CREATE TABLE "SE_ROLE_PERMISSION"
         (	"ID" NUMBER(10,0)  IDENTITY PRIMARY KEY,
      	"CREATED_TIME" DATE,
      	"STATUS" NUMBER(1,0),
	"ROLE" VARCHAR2(50 CHAR) ,
	"PERMISSION" VARCHAR2(512 CHAR) ,
FOREIGN KEY ( ROLE ) REFERENCES SE_ROLE( NAME ),
FOREIGN KEY ( PERMISSION ) REFERENCES SE_PERMISSION( NAME )

         )


    CREATE TABLE "SE_USER_ROLE"
         (	"ID" NUMBER(10,0)  IDENTITY PRIMARY KEY,
      	"CREATED_TIME" DATE,
      	"STATUS" NUMBER(1,0),
	"ROLE" VARCHAR2(50 CHAR) ,
	"USER_NAME" VARCHAR2(512 CHAR) ,
FOREIGN KEY ( ROLE ) REFERENCES SE_ROLE( NAME ),
FOREIGN KEY ( USER_NAME ) REFERENCES AM_USER( USER_NAME )

         )
