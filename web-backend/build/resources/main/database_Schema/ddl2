CREATE TABLE AM_USER
(
    USER_ID integer PRIMARY KEY NOT NULL,
    ADDRESS varchar(512),
    AUTH_METHOD varchar(20),
    CREATED_ID integer,
    CREATED_TIME date,
    DELETED_ID integer,
    DOCUMENT varchar(255),
    EMAIL varchar(50),
    EXPIRE_STATUS decimal(19,2),
    FAILURE_COUNT integer,
    FAX varchar(15),
    FULL_NAME varchar(100),
    LOCKED_DATE date,
    MOBILE varchar(15),
    MODIFIED_ID integer,
    MODIFIED_PASSWORD date,
    PASSWORD varchar(100),
    PASSWORD_BACKUP varchar(50),
    PHONE varchar(15),
    PRIORITY integer,
    RIGHT_TYPE integer,
    STATUS decimal(19,2),
    USER_NAME varchar(50)
);
CREATE TABLE ROLE_PERMISSION
(
    ROLE varchar(512) NOT NULL,
    PERMISSION varchar(512) NOT NULL,
    CONSTRAINT CONSTRAINT_9 PRIMARY KEY (ROLE, PERMISSION),
    CONSTRAINT FKO7CF2IU49BJAX2TUUKCTA3AK3 FOREIGN KEY (ROLE) REFERENCES SE_ROLE (NAME),
    CONSTRAINT FK9535P009CNDJ407PIA3CL7DFR FOREIGN KEY (PERMISSION) REFERENCES SE_PERMISSION (NAME)
);
CREATE TABLE SE_OPERATION
(
    CODE varchar(30) PRIMARY KEY NOT NULL,
    CREATED_TIME date,
    NAME varchar(512),
    STATUS decimal(19,2)
);
CREATE TABLE SE_PERMISSION
(
    NAME varchar(512) PRIMARY KEY NOT NULL,
    CREATED_TIME date,
    STATUS decimal(19,2),
    OPERATION varchar(30),
    RESOURCE varchar(512),
    CONSTRAINT FKKMU7DYLBMAM0MWFDVL1CSE8ND FOREIGN KEY (OPERATION) REFERENCES SE_OPERATION (CODE),
    CONSTRAINT FK8US1WY74Y5XO5GJGGM1TE5OEX FOREIGN KEY (RESOURCE) REFERENCES SE_RESOURCE (NAME)
);
CREATE TABLE SE_RESOURCE
(
    NAME varchar(512) PRIMARY KEY NOT NULL,
    CREATED_TIME date,
    STATUS decimal(19,2),
    PARENT varchar(512),
    CONSTRAINT CONSTRAINT_3D FOREIGN KEY (PARENT) REFERENCES SE_RESOURCE (NAME)
);
CREATE TABLE SE_ROLE
(
    NAME varchar(512) PRIMARY KEY NOT NULL,
    CREATED_TIME date,
    DESCRIPTION varchar(512),
    STATUS decimal(19,2)
);
CREATE TABLE SE_ROLE_PERMISSION
(
    ROLE varchar(512) NOT NULL,
    PERMISSION varchar(512) NOT NULL,
    CONSTRAINT CONSTRAINT_C PRIMARY KEY (ROLE, PERMISSION),
    CONSTRAINT FKKTWDF28BWAAA6XPFJCPJ2GA FOREIGN KEY (ROLE) REFERENCES SE_ROLE (NAME),
    CONSTRAINT FKAD49TB5IMPIV4DJJOD1PEE08T FOREIGN KEY (PERMISSION) REFERENCES SE_PERMISSION (NAME)
);
CREATE TABLE SE_USER_ROLE
(
    USER integer NOT NULL,
    ROLE varchar(512) NOT NULL,
    CONSTRAINT CONSTRAINT_39 PRIMARY KEY (USER, ROLE),
    CONSTRAINT FK9YMSFIIHF2SH7HRRD2K0HBYLO FOREIGN KEY (USER) REFERENCES AM_USER (USER_ID),
    CONSTRAINT FKIQNEPC03V5IY33Q01CBF5F2GB FOREIGN KEY (ROLE) REFERENCES SE_ROLE (NAME)
);
CREATE TABLE USER_ROLE
(
    USER integer NOT NULL,
    ROLE varchar(512) NOT NULL,
    CONSTRAINT CONSTRAINT_B PRIMARY KEY (USER, ROLE),
    CONSTRAINT FK4PPV7UG9Y7XCCO1WGVT4NW82H FOREIGN KEY (USER) REFERENCES AM_USER (USER_ID),
    CONSTRAINT FK9B9GBK22VJJ6QJGLRXGVL4B8N FOREIGN KEY (ROLE) REFERENCES SE_ROLE (NAME)
)CREATE TABLE AM_USER
(
    USER_ID integer PRIMARY KEY NOT NULL,
    ADDRESS varchar(512),
    AUTH_METHOD varchar(20),
    CREATED_ID integer,
